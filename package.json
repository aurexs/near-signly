{
  "name": "signear",
  "version": "0.3.4",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build-ui": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build": "yarn build:asb",
    "build:asb": "asb",
    "build:asb:debug": "asb --target debug",
    "deploy": "yarn build && near deploy",
    "dev-deploy": "yarn build:asb:debug && near dev-deploy",
    "clean": "rm -rf ./out && rm -f ./neardev/dev-account*",
    "test": "yarn build:asb:debug && asp",
    "test2": "yarn build:asb:debug && asp && jest",
    "accounts": "./near-account-utils/bin/accounts-report",
    "keys": "./near-account-utils/bin/keys-report",
    "storage": "./near-account-utils/bin/storage-report",
    "sk2pk": "./near-account-utils/bin/private-to-public-keys"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.22",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "chalk": "^4.0.0",
    "chardet": "^1.1.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "got": "^11.1.3",
    "near-cli": "^3.2.0",
    "near-sdk-as": "^3.2.3",
    "prettier": "^2.5.1",
    "table": "^5.4.6",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vue-tsc": "^0.31.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.2",
    "axios": "^0.26.0",
    "big.js": "^6.1.1",
    "bootstrap": "^5.1.3",
    "buffer": "^6.0.3",
    "env-cmd": "^10.1.0",
    "near-api-js": "^0.44.2",
    "pinia": "^2.0.11",
    "prop-types": "^15.8.1",
    "vue": "^3.2.29",
    "vue-router": "^4.0.12",
    "yup": "^0.32.11"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  }
}
